#!/bin/sh
# Cisco ASDM Launcher

A=$dotfiles/asdm

exec /usr/lib/jvm/java-6-oracle/bin/java -Xms64m -Xmx512m -XX:MaxNewSize=1024k \
    -Dsun.swing.enableImprovedDragGesture=true \
    -Dapple.laf.useScreenMenuBar=true \
    -Dapple.awt.graphics.UseQuartz=true \
    -classpath ${A}/lzma.jar:${A}/jploader.jar:${A}/asdm-launcher.jar:${A}/retroweaver-rt-2.0.7.jar \
    com.cisco.launcher.Launcher
