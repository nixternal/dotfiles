" :h '<option>' <- to see what each of these does
filetype plugin on
set autoindent
set background=dark
set backspace=2
set complete=.,w,b,u,t,i
set completeopt=longest,menuone,preview
set confirm
set encoding=utf-8
set expandtab
set foldlevelstart=0
set foldmethod=marker
set grepprg=grep\ -nH\ $*
set guitablabel=%N/\ %t\ %M
set history=100
set hlsearch
set ignorecase
set incsearch
set laststatus=2
set lazyredraw
set matchtime=2
set modeline
set modelines=5
set mousehide
set nocp
set noshowmode
set notimeout
set relativenumber
set numberwidth=1
set omnifunc=syntaxcomplete#Complete
set path+=**
set printoptions=header:0,duplex:long,paper:letter
set report=0
set ruler
set scrolloff=3
set shiftwidth=4
set shortmess+=a
set showcmd
set showfulltag
set showmatch
set smartcase
set softtabstop=4
set splitbelow
set splitright
set synmaxcol=800
set tabstop=4
set t_Co=256
set textwidth=80
set title
set ttimeout
set ttimeoutlen=10
set ttyfast
set vb t_vb=
set wrapscan

" vim:ft=vim
